{
  "name": "ring-assist-twilio",
  "version": "1.0.0",
  "description": "Minimal boilerplate for Node.js, TypeScript, Express projects",
  "type": "module",
  "scripts": {
    "start": "node --watch dist/src/app/entrypoints/index.mjs",
    "clean": "rimraf coverage dist tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint .",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch --runInBand",
    "prisma:migrate": "prisma migrate dev"
  },
  "imports": {
    "#app/*": "./dist/src/app/*",
    "#__tests__/*": "./dist/__tests__/*"
  },
  "prisma": {
    "schema": "src/app/adapters/orm/prisma/schema.prisma"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.34.0",
    "@google-cloud/speech": "^6.7.0",
    "@google-cloud/text-to-speech": "^5.6.0",
    "@google-cloud/vertexai": "^1.9.0",
    "@prisma/client": "6.0.1",
    "@types/body-parser": "^1.19.5",
    "@types/fluent-ffmpeg": "^2.1.27",
    "axios": "^1.7.8",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.6",
    "express": "5.0.0",
    "fluent-ffmpeg": "^2.1.3",
    "install": "^0.13.0",
    "modesl": "^1.2.1",
    "morgan": "^1.10.0",
    "ngrok": "5.0.0-beta.2",
    "npm": "^10.9.1",
    "openai": "^4.74.0",
    "tslib": "~2.8.1",
    "twilio": "^5.3.6",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/modesl": "^1.1.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "prisma": "^6.0.1",
    "ts-api-utils": "^2.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "license": "Apache-2.0"
}
